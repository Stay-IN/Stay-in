{
  "name": "stayin",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "10.x",
    "npm": "6.x",
    "yarn": "1.x"
  },
  "cacheDirectories": [
    "backend/node_modules",
    "portal/node_modules"
  ],
  "scripts": {
    "portal": "cd portal && npm start && cd ..",
    "install": "npm install && cd portal && npm install && cd .. && cd backend && npm install && cd ..",
    "backend": "cd backend && npm start && cd ..",
    "start": "npm run start:backend",
    "build:portal": "cd portal && yarn build && cd --",
    "build": "npm run build:portal && cd --",
    "stayin": "concurrently \"npm run portal\" \"npm run backend\""
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "concurrently": "^5.1.0",
    "express-firebase": "^1.1.1"
  }
}
